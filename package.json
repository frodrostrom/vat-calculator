{
  "name": "foxit_aec_e2e",
  "private": "true",
  "scripts": {
    "auth": "cross-env REAUTH=1 playwright test --pass-with-no-tests abc123xyz",
    "fmt": "run-s fmt:*",
    "fmt:eslint": "eslint . --fix",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint . --max-warnings 0",
    "lint:tsc": "tsc",
    "test": "playwright test",
    "test:all": "run-s auth test:default-args test:all:run",
    "test:all:browser:chromium": "cross-var playwright test --browser chromium --output test-results-chromium $npm_config_args",
    "test:all:browser:firefox": "cross-var playwright test --browser firefox --output test-results-firefox $npm_config_args",
    "test:all:browser:webkit": "cross-var playwright test --browser webkit --output test-results-webkit $npm_config_args",
    "test:all:run": "cross-env REAUTH=0 run-s test:all:browser:* --continue-on-error",
    "test:ci": "cross-env CI=1 run-s test:default-args test:ci:run test:ci:status",
    "test:ci:run": "cross-var exitzero playwright test $npm_config_args",
    "test:ci:status": "node setup/pipeline-status.js",
    "test:default-args": "npm config set args \"\""
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@playwright/test": "^1.29.2",
    "async-retry": "^1.3.3",
    "brotli": "^1.3.3",
    "date-fns": "^2.29.3",
    "deepmerge": "^4.2.2",
    "dotenv": "^16.0.3",
    "fast-xml-parser": "^4.0.14",
    "glob": "^8.1.0",
    "mime-types": "^2.1.35",
    "ordinal": "^1.0.3",
    "p-parallel": "^1.0.3",
    "pixelmatch": "^5.3.0",
    "playwright": "^1.49.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@types/async-retry": "^1.4.5",
    "@types/brotli": "^1.3.1",
    "@types/glob": "^8.0.1",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^18.11.18",
    "@types/pngjs": "^6.0.1",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "cross-env": "^7.0.3",
    "cross-var-no-babel": "^1.2.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-no-relative-import-paths": "^1.5.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^45.0.2",
    "exitzero": "^1.0.1",
    "jpeg-js": "^0.4.4",
    "npm-run-all": "^4.1.5",
    "pngjs": "^6.0.0",
    "prettier": "^2.8.3"
  }
}
